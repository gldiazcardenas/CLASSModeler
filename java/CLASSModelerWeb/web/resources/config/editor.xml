<mxEditor urlImage  = "/CLASSModeler/pages/designer/servlet?image"
          urlInit   = "/CLASSModeler/pages/designer/servlet?init"
          urlPoll   = "/CLASSModeler/pages/designer/servlet?poll"
          urlNotify = "/CLASSModeler/pages/designer/servlet?notify"
          urlPost   = "/CLASSModeler/pages/designer/servlet?save">
          
    <ui>
        <!-- <resource basename="resources/messages"/> -->
        <add as="graph" element="graph"/>
        <add as="toolbar" element="toolbox"/>
    </ui>
    
    <!-- Templates -->
    <Array as="templates">
        <add as="class">
            <Class name="Clase" visibility="public" abstract="0" static="0" final="0" package="">
                <mxCell vertex="1" style="classifier">
                    <mxGeometry as="geometry" width="0" height="20" />
                </mxCell>
            </Class>
        </add>
        
        <add as="interface">
            <Interface name="Interfaz" visibility="public" package="">
                <mxCell vertex="1" style="classifier">
                    <mxGeometry as="geometry" width="0" height="20" />
                </mxCell>
            </Interface>
        </add>
        
        <add as="enumeration">
            <Enumeration name="Enumeracion" visibility="public" package="">
                <mxCell vertex="1" style="classifier">
                    <mxGeometry as="geometry" width="0" height="20" />
                </mxCell>
            </Enumeration>
        </add>
        
        <add as="package">
            <Package name="paquete">
                <mxCell vertex="1" style="package">
                    <mxGeometry as="geometry" width="0" height="0" />
                </mxCell>
            </Package>
        </add>
        
        <add as="comment">
            <Comment body=":: insertar texto ::">
                <mxCell vertex="1" style="comment">
                    <mxGeometry as="geometry" width="0" height="0" />
                </mxCell>
            </Comment>
        </add>
        
        <add as="section">
            <Section attribute="1">
                <mxCell vertex="1" connectable="0" style="section">
                    <mxGeometry as="geometry" width="0" height="20" />
                </mxCell>
            </Section>
        </add>
        
        <add as="property">
            <Property name="" visibility="private" static="0" final="0" initialValue="" type="" collection="" upper="" lower="">
                <mxCell vertex="1" connectable="0" style="feature">
                    <mxGeometry as="geometry" width="0" height="20" />
                </mxCell>
            </Property>
        </add>
        
        <add as="operation">
            <Operation name="" visibility="public" static="0" final="0" abstract="0" synchronized="0" type="" collection="">
                <mxCell vertex="1" connectable="0" style="feature">
                    <mxGeometry as="geometry" width="0" height="20" />
                </mxCell>
            </Operation>
        </add>
        
        <add as="parameter">
            <Parameter name="" type="" collection="">
                <mxCell vertex="1">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Parameter>
        </add>
        
        <add as="literal">
            <Literal name="">
                <mxCell vertex="1" connectable="0" style="feature">
                    <mxGeometry as="geometry" width="0" height="20" />
                </mxCell>
            </Literal>
        </add>
        
        <add as="association">
            <Association name="" aggregation="none">
                <mxCell edge="1" style="association">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Association>
        </add>
        
        <add as="aggregation">
            <Association name="" aggregation="shared">
                <mxCell edge="1" style="aggregation">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Association>
        </add>
        
        <add as="composition">
            <Association name="" aggregation="composite">
                <mxCell edge="1" style="composition">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Association>
        </add>
        
        <add as="generalization">
            <Generalization name="">
                <mxCell edge="1" style="generalization">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Generalization>
        </add>
        
         <add as="realization">
            <Realization name="">
                <mxCell edge="1" style="realization">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Realization>
        </add>
        
        <add as="dependency">
            <Dependency name="">
                <mxCell edge="1" style="dependency">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Dependency>
        </add>
        
        <add as="link">
            <Link>
                <mxCell edge="1" style="link">
                    <mxGeometry as="geometry" relative="1" />
                </mxCell>
            </Link>
        </add>
        
    </Array>
    
    <!-- Graph -->
    <mxGraph as="graph">
        <mxStylesheet as="stylesheet">
            <add as="defaultVertex">
                <add as="align" value="left"/>
                <add as="verticalAlign" value="middle"/>
                <add as="fontSize" value="11"/>
            </add>
            
            <add as="defaultEdge">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="elbow" value="horizontal"/>
            </add>
            
            <add as="classifier">
                <add as="shape" value="swimlane"/>
                <add as="perimeter" value="rectanglePerimeter"/>
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="gradientColor" value="#DDDDDD"/>
                <add as="fillColor" value="#EEEEEE"/>
                <add as="swimlaneFillColor" value="#FFFFFF"/>
                <add as="strokeColor" value="#AAAAAA"/>
                <add as="strokeWidth" value="1"/>
                <add as="fontColor" value="#000000"/>
                <add as="fontStyle" value="1"/>
                <add as="spacingLeft" value="20"/>
                <add as="spacingRight" value="20"/>
            </add>
            
            <add as="feature">
                <add as="shape" value="rectangle"/>
                <add as="perimeter" value="RectanglePerimeter"/>
                <add as="align" value="left"/>
                <add as="verticalAlign" value="middle"/>
                <add as="fontColor" value="#000000"/>
                <add as="fontStyle" value="0"/>
                <add as="spacingLeft" value="2"/>
                <add as="resizable" value="0"/>
                <add as="movable" value="0"/>
            </add>
            
            <add as="package">
                <add as="shape" value="package"/>
                <add as="perimeter" value="rectanglePerimeter"/>
                <add as="strokeColor" value="#AAAAAA"/>
                <add as="fillColor" value="#FFFFFF"/>
                <add as="align" value="center"/>
                <add as="spacingLeft" value="20"/>
                <add as="spacingRight" value="20"/>
                <add as="spacingTop" value="30"/>
                <add as="spacingBottom" value="20"/>
            </add>
            
            <add as="comment">
                <add as="shape" value="comment"/>
                <add as="perimeter" value="rectanglePerimeter"/>
                <add as="strokeColor" value="#BBBBBB"/>
                <add as="fillColor" value="#FFFFFF"/>
                <add as="align" value="left"/>
                <add as="spacingLeft" value="5"/>
                <add as="spacingRight" value="5"/>
                <add as="spacingTop" value="5"/>
                <add as="spacingBottom" value="5"/>
            </add>
            
            <add as="section">
                <add as="shape" value="swimlane"/>
                <add as="perimeter" value="RectanglePerimeter"/>
                <add as="align" value="right"/>
                <add as="verticalAlign" value="middle"/>
                <add as="strokeColor" value="#AAAAAA"/>
                <add as="strokeWidth" value="1"/>
                <add as="fillColor" value="#FFFFFF"/>
                <add as="startSize" value="0"/>
                <add as="fontStyle" value="0"/>
                <add as="shadow" value="0"/>
                <add as="foldable" value="0"/>
                <add as="resizable" value="0"/>
                <add as="movable" value="0"/>
            </add>
            
            <add as="association">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="elbow" value="horizontal"/>
                <add as="endSize" value="8"/>
                <add as="labelBackgroundColor" value="#FFFFFF" />
                <add as="labelPadding" value="2" />
            </add>
            
            <add as="aggregation">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="elbow" value="horizontal"/>
                <add as="startArrow" value="diamond"/>
                <add as="startSize" value="12"/>
                <add as="startFill" value="0"/>
                <add as="endSize" value="8"/>
                <add as="labelBackgroundColor" value="#FFFFFF" />
                <add as="labelPadding" value="2" />
            </add>
            
            <add as="composition">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="startArrow" value="diamond"/>
                <add as="startSize" value="12"/>
                <add as="startFill" value="1"/>
                <add as="endSize" value="8"/>
                <add as="labelBackgroundColor" value="#FFFFFF" />
                <add as="labelPadding" value="2" />
            </add>
            
            <add as="generalization">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="elbow" value="horizontal"/>
                <add as="endArrow" value="block"/>
                <add as="endSize" value="11"/>
                <add as="endFill" value="0"/>
                <add as="labelBackgroundColor" value="#FFFFFF" />
                <add as="labelPadding" value="2" />
            </add>
            
            <add as="realization">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="elbow" value="horizontal"/>
                <add as="endArrow" value="block"/>
                <add as="endSize" value="11"/>
                <add as="endFill" value="0"/>
                <add as="dashed" value="1"/>
                <add as="labelBackgroundColor" value="#FFFFFF" />
                <add as="labelPadding" value="2" />
            </add>
            
            <add as="dependency">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="elbow" value="horizontal"/>
                <add as="endArrow" value="open"/>
                <add as="endSize" value="8"/>
                <add as="dashed" value="1"/>
                <add as="labelBackgroundColor" value="#FFFFFF" />
                <add as="labelPadding" value="2" />
            </add>
            
            <add as="link">
                <add as="align" value="center"/>
                <add as="verticalAlign" value="middle"/>
                <add as="rounded" value="1"/>
                <add as="strokeColor" value="#000000"/>
                <add as="fontColor" value="#000000"/>
                <add as="edgeStyle" value="orthogonalEdgeStyle"/>
                <add as="elbow" value="horizontal"/>
                <add as="dashed" value="1"/>
                <add as="editable" value="0"/>
            </add>
            
        </mxStylesheet>
    </mxGraph>
    
    <!-- Key Handler -->
    <mxDefaultKeyHandler as="keyHandler">
        <add as="37" action="moveLeft"/>
        <add as="38" action="moveUp"/>
        <add as="39" action="moveRight"/>
        <add as="40" action="moveDown"/>
        <add as="46" action="delete"/>
        <add as="65" control="1" action="selectAll"/>
        <add as="90" control="1" action="undo"/>
        <add as="89" control="1" action="redo"/>
        <add as="88" control="1" action="cut"/>
        <add as="67" control="1" action="copy"/>
        <add as="86" control="1" action="paste"/>
        <add as="71" control="1" action="save"/>
    </mxDefaultKeyHandler>
    
</mxEditor>